{
    "name": "app",
    "type": "module",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "build:webpack": "TURBOPACK_DISABLED=1 next build",
        "build:check": "npm run typecheck && npm run lint && npm run build",
        "start": "next start",
        "typecheck": "tsc --noEmit",
        "lint": "npx eslint . --ext .ts,.tsx,.js,.jsx",
        "lint:fix": "npx eslint . --ext .ts,.tsx,.js,.jsx --fix",
        "test": "vitest run",
        "test:unit": "vitest run",
        "test:cov": "vitest run --coverage",
        "test:watch": "vitest",
        "test:integration": "vitest run tests/integration --reporter verbose",
        "test:e2e": "vitest run tests/e2e --reporter verbose",
        "test:redteam": "vitest run redteam/tests/ --reporter verbose",
        "test:redteam:ci": "vitest run redteam/tests/ --reporter=json --outputFile=redteam-results.json",
        "test:security": "vitest run tests/rag-security.test.ts redteam/tests/ --reporter verbose",
        "test:supabase": "node scripts/test-supabase.mjs",
        "test:groq": "node scripts/test-groq.mjs",
        "write-env": "powershell -ExecutionPolicy Bypass -NoProfile -File scripts/write-env.ps1",
        "diagnose:vitest": "node ./scripts/diagnose-vitest.mjs",
        "evaluate:rag": "npx ts-node --esm scripts/run-rag-evaluation.ts evaluate",
        "evaluate:rag:ci": "python python/evaluate_rag.py evaluate --ci --samples 5",
        "evaluate:rag:full": "python python/evaluate_rag.py evaluate --samples 100 --verbose",
        "evaluate:rag:generate": "npx ts-node --esm scripts/run-rag-evaluation.ts generate",
        "evaluate:rag:sample": "python python/evaluate_rag.py create-sample-data",
        "test:ci": "vitest run --reporter=json --outputFile=test-results.json",
        "test:onboarding": "vitest run tests/ContinueWithLoader.test.ts tests/MultiStepLoader.test.ts --reporter verbose",
        "test:perf": "npx tsx tests/performance/embedding-pipeline.perf.ts",
        "simulate:ingest": "node scripts/dev-simulate-ingest.js",
        "reindex:vectors": "node scripts/reindex_vectors.js",
        "bench:redis": "npx tsx scripts/benchmark-redis.ts",
        "bench:hf": "npx tsx scripts/benchmark-hf.ts",
        "bench:all": "npm run bench:redis && npm run bench:hf",
        "rollout:status": "npx tsx scripts/manage-rollout.ts status",
        "rollout:increase": "npx tsx scripts/manage-rollout.ts increase",
        "rollout:int8": "npx tsx scripts/manage-rollout.ts enable-int8",
        "rollout:pilot": "npx tsx scripts/manage-rollout.ts pilot",
        "scheduler:int8": "npx tsx scripts/scheduler-int8.ts",
        "scheduler:int8:watch": "npx tsx scripts/scheduler-int8.ts --continuous",
        "redteam:test": "npx tsx redteam/test-runner.ts",
        "redteam:report": "npm run redteam:test -- 2>/dev/null | tail -100",
        "safety:rerank": "npx tsx -e \"import { retrieveWithSafetyReRanking } from '@/lib/safety/rrf-reranker'; console.log('Re-ranker loaded');\"",
        "safety:compare": "npx tsx scripts/compare-safety-reports.ts",
        "test:safety": "vitest run tests/safety/",
        "worker:tenant-pipeline": "tsx src/workers/tenantPipelineWorker.ts"
    },
