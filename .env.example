# Google Cloud Text-to-Speech API Key
# Get your API key from: https://console.cloud.google.com/apis/credentials
GOOGLE_TTS_API_KEY=

# Optional: TTS Cache Configuration
TTS_CACHE_MAX_ITEMS=200
TTS_CACHE_TTL_MS=86400000
TTS_RATE_LIMIT_WINDOW_MS=60000
TTS_RATE_LIMIT_MAX=6

# LLM provider (default: groq)
BITB_LLM_PROVIDER=groq
BITB_LLM_MODEL=llama-3.1-70b-instruct
GROQ_API_KEY=
GROQ_BASE_URL=https://api.groq.com/openai/v1
